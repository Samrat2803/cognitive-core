option_settings:
  # Essential: Tell EB how to start the app - Fixed for FastAPI with ASGI  
  aws:elasticbeanstalk:container:python:
    WSGIPath: application:application
    
  # Essential: Set environment variables
  # NOTE: These values should be configured in AWS EB Environment Configuration
  # Do NOT put real API keys or secrets in this file
  aws:elasticbeanstalk:application:environment:
    # API Keys - Set these in EB Console -> Configuration -> Environment Properties
    # TAVILY_API_KEY: [set via EB console]
    # OPENAI_API_KEY: [set via EB console] 
    # MONGODB_CONNECTION_STRING: [set via EB console]
    DATABASE_NAME: web_research_agent
    CORS_ORIGINS: "http://localhost:3000,https://dgbfif5o7v03y.cloudfront.net"
